    <h1 class="page-header">Users</h1>

    <div class="row placeholders">
    	<table class=" table table-striped">
        <thead>
      		<tr = "users_row">
      			<th>Email</th>
            <th>Sign-in Count </th>
            <th>Last Sign-in </th>
      			<th class = "lastlinks">Actions</th>
      		</tr>
        </thead>
        <tbody>
      		<% @users.each do |user| %>
      			<tr class = "tablerow">
      				<td><%= user.email %></td>
              <td><%= user.sign_in_count %> </td>
              <td><%= user.last_sign_in_at.strftime("%B %e, %Y %H:%M") if user.last_sign_in_at? %> </td>
      				<td class = "lastlinks">
               <%= link_to admin_user_path(user) do %> <i class = "glyphicon glyphicon-info-sign"></i> <%end %>
               <%= link_to edit_admin_user_path(user) do %> <i class = "glyphicon glyphicon-pencil"> </i> <% end %>
               <%= link_to admin_user_path(user), method: :delete, confirm: 'Are you certain you want to delete this?' do %> <i class = "glyphicon glyphicon-remove"> </i> <% end %>
             </td>
      			</tr>
      		<% end %>
        </tbody>
    	</table>
    </div>
